{
  "name": "my-rollup",
  "version": "1.0.0",
  "scripts": {
    "rollup": "rollup -c rollup.config.js",
    "build:iife": "MODULE_TYPE=iife npm run rollup",
    "build:cjs": "MODULE_TYPE=cjs npm run rollup",
    "build:esm": "MODULE_TYPE=esm npm run rollup",
    "build:umd": "MODULE_TYPE=umd npm run rollup",
    "build": "npm run build:iife && npm run build:cjs && npm run build:esm && npm run build:umd"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "rollup": "^2.33.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  }
}
